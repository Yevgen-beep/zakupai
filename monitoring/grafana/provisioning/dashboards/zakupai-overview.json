{
  "uid": "zakupai-overview",
  "title": "ZakupAI Stage6 Overview",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {"from": "now-6h", "to": "now"},
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Ошибки 5xx по сервисам",
      "datasource": {"type": "prometheus", "uid": "zakupai-prom"},
      "targets": [
        {"expr": "sum by (service) (rate(http_requests_total{status=~\"5..\"}[5m]))", "legendFormat": "{{service}}", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "reqps"}},
      "options": {"legend": {"displayMode": "table", "placement": "right"},"tooltip": {"mode": "multi","sort": "desc"}},
      "gridPos": {"h": 9, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Антидемпинг >15%",
      "datasource": {"type": "prometheus", "uid": "zakupai-prom"},
      "targets": [
        {"expr": "zakupai_antidumping_rate", "legendFormat": "Antidumping rate", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "percentunit"}},
      "options": {"legend": {"displayMode": "table", "placement": "right"}, "tooltip": {"mode": "single"}},
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 0}
    }
  ],
  "templating": {"list": []},
  "annotations": {"list": [{"builtIn": 1, "datasource": {"type": "grafana","uid": "-- Grafana --"}, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard"}]}
}
